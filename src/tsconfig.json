{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "sourceMap": true,
        "noEmitHelpers": true,
        "noEmitOnError": false,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "removeComments": true,
        "noLib": false,
        "declaration": true,
        "pretty": true
    },
    "compileOnSave": false
}